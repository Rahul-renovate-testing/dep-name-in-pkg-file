{
  "customManagers": [{
    "customType": "regex",
    "managerFilePatterns": ["/^packages\/.*/"],
    "matchStrings": ["(?<currentValue>.*)"],
    "depNameTemplate": ["{{packageFile}}"],
    "datasourceTemplate": "npm"
  },
  {
    "customType": "regex",
    "managerFilePatterns": ["/^also-packages\/.*\/service.env$/"],
    "matchStrings": ["(?<currentValue>.*)"],
    "depNameTemplate": ["{{lookup (split packageFileDir '/') 1}}"],
    "datasourceTemplate": "npm"
  }
  ]
}
